<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>

    <ui:with field="res"
          type="com.github.gwt.user.client.ui.resources.ClearBoxResourcesImpl"/>

    <g:HorizontalPanel styleName="{res.clearBoxResources.style.correct}"
                       verticalAlignment="ALIGN_MIDDLE">
        <g:ValueBoxBase ui:field="valueBox"
                        styleName="{res.clearBoxResources.style.empty}"/>
        <g:FocusWidget ui:field="clearButton"
                       styleName="{res.clearBoxResources.style.clearButton}"/>
    </g:HorizontalPanel>

</ui:UiBinder>